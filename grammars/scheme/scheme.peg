
package main

type Parser Peg {
 
}

Start
    <- (Comment / List Comment?)* eof

List       
    <-  begin_paren Operator (spacing* Value)* end_paren

Value 
    <- True
       / False
       / Null
       / Character
       / String
       / Number
       / List

Comment
    <- ws* ';' (!eol .)* eol

Operator
    <- char*

True   
    <- "#t" 

False  
    <- "#f" 

Null   
    <- "nil" 

String 
    <- doublequote char* doublequote

Character
    <- "#\\" char

char   
    <- (!doublequote !ws !Keywords .)

Number 
    <- '0'
       / [1-9] Digit* ('.' Digit*)?

Digit  
    <- [0-9]

Keywords
    <- [;#]

begin_paren     <- spacing* "(" spacing*
end_paren       <- spacing* ")" spacing*
ws              <- [ \r\n\t]
spacing         <- ws / Comment
backslash       <- '\\'
doublequote     <- '"'
eol             <- '\n'
eof             <- !.
