
package main

type Parser Peg {
 
}


Start
    <- (Comment / ListWithComment)* eof

ListWithComment
    <- List Comment?

List       
    <-  begin_paren Operator (ws* Value)* end_paren

Value 
    <- True
       / False
       / Null
       / String
       / Number
       / List

Comment
    <- space* ';' (!eol .)* ws*

Operator
    <- Char*

True   
    <- "#t" 

False  
    <- "#f" 

Null   
    <- "nil" 

String 
    <- doublequote Char* doublequote 

Char   
    <- (!doublequote !ws !Keywords .)

Number 
    <- '0'
       / [1-9] Digit* ('.' Digit*)?

Digit  
    <- [0-9]

Keywords
    <- [;]

begin_paren     <- ws* "(" ws*
end_paren       <- ws* ")" ws*
ws              <- [ \r\n\t]
space           <- [ \r\t]
backslash       <- '\\'
doublequote     <- '"'
eol             <- '\n'
eof             <- !.
